{
  "name": "react-native-virtual-scroll",
  "version": "0.0.1",
  "description": "React Native Virtual Scroll",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Petrosyan2001/react-native-virtual-scroll.git"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "virtual-scroll",
    "big-list",
    "react-native-virtual-scroll",
    "large-list"
  ],
  "author": "Petros Petrosyan (petrosyan.petros.2001@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-native": "^0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.71.4",
    "react-native-typescript-transformer": "^1.2.13",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@tsconfig/react-native": "^3.0.2",
    "jest": "^29.6.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-safe-area-view": "^1.1.1"
  }
}
